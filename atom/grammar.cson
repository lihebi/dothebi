'scopeName': 'source'
'name': 'HEBI'
'injectionSelector': 'comment, text.plain'
# 'fileTypes': ['hebi'] HEBI TODO FIXME HEBI PDF: fd UNIV
'patterns': [
  {
    'match': '(?<!\\w)@?(HEBI|CONF|PROBLEM|CONTRIB|DOWN|TECHNIQUE|UNIV|ALIAS)\\b'
    'name': 'hebi.type.${1:/downcase}'
  }
  {
    'match': 'PDF: [^\\s]+'
    'name': 'hebi.type.pdf'
  }
]
